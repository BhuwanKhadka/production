<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="ir_cron_attendance_sheet_get_attendance_cron"
                model="ir.cron">
            <field name="name">Update Attendance Sheet Attendance</field>
            <field name="active" eval="True"/>
            <field name="interval_number">7</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="state">code</field>
            <field name="code">model.cron_update_attendance_sheet()</field>
            <field name="doall" eval="False"/>
            <field name="model_id"
                   ref="rm_hr_attendance_sheet.model_attendance_sheet"/>
        </record>


    </data>
</odoo>